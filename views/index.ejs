<!DOCTYPE html>
<html lang="es">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Floreria Aloha</title>
</head>
<body>
    <h2>Galeria</h2>
<div>
    <% flores.forEach(function(flores) { %>
    <div class="card" style="width: 18rem;">
        <img src="flores[i].img" class="card-img-top" alt="...">
        <div class="card-body">
          <h5 class="card-title">flores[i].nombre</h5>
          <p class="card-text">flores[i].precio</p>
          <p class="card-text">flores[i].color</p>
        </div>
      </div>
      % }); %>
</div>
      
</body>
</html>